{
  "name": "eslint_precommit",
  "version": "1.0.0",
  "description": "basic node.js project: jest+eslint with precommit hook",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "keywords": [],
  "author": "Emil (Suleman) Shaiakhmeodov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@jest/core": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^29.7.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.2"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "jest-jasmine2": "^29.7.0",
    "pre-commit": "^1.2.2"
  }
}
